import matplotlib.pyplot as plt

# Sample Data
x = [1, 2, 3, 4]
y = [10, 20, 25, 30]

# Plotting the data
plt.plot(x, y, label='Line Chart', color='blue')

# Adding titles and labels
plt.title("Simple Line Chart")
plt.xlabel("X-axis")
plt.ylabel("Y-axis")
plt.legend()

# Display the graph
plt.show()